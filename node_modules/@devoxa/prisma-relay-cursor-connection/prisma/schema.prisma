datasource sqlite {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Todo {
  id          String  @id
  text        String
  isCompleted Boolean @default(false)
}

model User {
  id    Int    @id
  email String @unique
}

model Profile {
  firstname String
  lastname  String

  @@unique([firstname, lastname])
}
